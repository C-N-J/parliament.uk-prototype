{
  "containerDefinitions": [
    {
      "name": "{{APP_NAME}}",
      "image": "{{IMAGE}}",
      "cpu": 20,
      "memory": 512,
      "essential": true,
      "command": [],
      "portMappings": [
        {
          "hostPort": 88,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "{{APP_NAME}}",
          "awslogs-region": "{{AWS_REGION}}"
        }
      }
    }
  ],
  "family": "{{APP_NAME}}"
}
