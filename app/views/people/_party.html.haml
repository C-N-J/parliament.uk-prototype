%li.list__item
  %h2= link_to(party.name, party_path(party.graph_id))
  %ol
    - party.party_memberships.each do |party_membership|
      %li
        = "#{l(party_membership.start_date, format: :default)}" unless party_membership.start_date.nil?
        = party_membership.end_date.nil? ? " to Present" : " to #{l(party_membership.end_date, format: :default)}"

