.section--primary
  .container
    %h1
      = "#{t('mps_and_lords')}"
      %span= link_to "- #{@parliament.date_range(date_format: '%Y')} #{t('parliament')} #{t('parliaments.a_to_z')}", parliament_path(@parliament.graph_id)

    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :parliament_members_a_z_letter_path, route_args: [@parliament.graph_id] }

%section#content
  .container
    = render partial: 'pugin/cards/person-list', locals: { people: @people }
