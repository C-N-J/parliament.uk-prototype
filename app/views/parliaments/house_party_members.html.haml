%h2= "#{@party.name} MPs between #{@parliament.date_range(date_format: '%Y')}"

.section--primary
  .container
    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :parliament_house_party_members_a_z_letter_path, route_args: [@parliament.graph_id, @house.graph_id, @party.graph_id] }

%ul
  - @people.each do |person|
    %li= link_to person.display_name, person_path(person.graph_id)
    - person.seat_incumbencies.each do |seat_incumbency|
      = link_to seat_incumbency.constituency.name, constituency_path(seat_incumbency.constituency.graph_id)
      = seat_incumbency.constituency.date_range
